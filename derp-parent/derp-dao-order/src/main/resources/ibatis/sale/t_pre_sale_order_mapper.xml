    <!-- 查询条件 -->
    <sql id='sql_where_page'>
        <where>
            <trim suffixOverrides='and'>
                <if test='merchantId!=null' > merchant_id= #{merchantId} and </if>
                <if test='code!=null and !"".equals(code)' >code= #{code} and </if>
                <if test='customerId!=null' > customer_id= #{customerId} and </if>
                <if test='state!=null and !"".equals(state)' >state= #{state} and </if>
                <if test='buId!=null' > bu_id= #{buId} and </if>
                <if test='outDepotId!=null' > out_depot_id= #{outDepotId} and </if>
                <if test='createDateStartDate!=null and !"".equals(createDateStartDate)' >
                    date_format(create_date,'%Y-%m-%d %H:%i:%S') &gt;= #{createDateStartDate} and
                </if>
                <if test='createDateEndDate!=null and !"".equals(createDateEndDate)' >
                    date_format(create_date,'%Y-%m-%d %H:%i:%S') &lt;= #{createDateEndDate} and
                </if>
                <if test='buList!=null and !"".equals(buList)' > bu_id in
                    <foreach collection='buList' item='buId' separator=',' open='(' close=')'>
                        #{buId}
                    </foreach>
                    and
                </if>
                <if test='ids!=null and !"".equals(ids)' >id in
                    <foreach collection="ids.split(',')"  item='id' separator=',' open='(' close=')'>
                        #{id}
                    </foreach>
                    and
                </if>
                state != "006"
            </trim>
        </where>
    </sql>

