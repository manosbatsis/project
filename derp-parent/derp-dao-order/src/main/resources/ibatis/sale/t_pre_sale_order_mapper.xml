    <sql id='sql_where_page1'>
        <where>
            <trim suffixOverrides='and'>
                <if test='merchantId!=null' > p.merchant_id= #{merchantId} and </if>
                <if test='code!=null and !"".equals(code)' >p.code= #{code} and </if>
                <if test='customerId!=null' > p.customer_id= #{customerId} and </if>
                <if test='state!=null and !"".equals(state)' >p.state= #{state} and </if>
                <if test='buId!=null' > p.bu_id= #{buId} and </if>
                <if test='outDepotId!=null' > p.out_depot_id= #{outDepotId} and </if>
                <if test='createDateStartDate!=null and !"".equals(createDateStartDate)' >
                    date_format(p.create_date,'%Y-%m-%d %H:%i:%S') &gt;= #{createDateStartDate} and
                </if>
                <if test='createDateEndDate!=null and !"".equals(createDateEndDate)' >
                    date_format(p.create_date,'%Y-%m-%d %H:%i:%S') &lt;= #{createDateEndDate} and
                </if>
                <if test='poNo!=null and !"".equals(poNo)' >p.po_no like CONCAT('%',#{poNo},'%')  and </if>
                <if test='buList!=null and !"".equals(buList)' >p.bu_id in
                    <foreach collection='buList' item='buId' separator=',' open='(' close=')'>
                        #{buId}
                    </foreach>
                    and
                </if>
                <if test='ids!=null and !"".equals(ids)' >p.id in
                    <foreach collection="ids.split(',')"  item='id' separator=',' open='(' close=')'>
                        #{id}
                    </foreach>
                    and
                </if>
                p.state != '006'
            </trim>
        </where>
    </sql>
    <!-- 查询所有信息 分页 -->
    <select id='queryDTOListByPage' parameterType='PreSaleOrderDTO' resultMap='PreSaleOrderDTOMap'>
        select
            p.id, p.code, p.customer_name,p.bu_name,p.out_depot_name, p.po_no, p.create_date,
            p.create_name,p.state,SUM(pi.num) num, SUM(pi.amount) amount
        from t_pre_sale_order p
        LEFT JOIN t_pre_sale_order_item pi on p.id = pi.order_id
        <include refid='sql_where_page1' />
        GROUP BY p.id, p.code, p.customer_name,p.bu_name,p.out_depot_name, p.po_no, p.create_date, p.create_name,p.state
        order by p.id desc
    </select>

